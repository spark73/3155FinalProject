<h2>Add to the Menu</h2>
<%= form_for([@restaurant, @food]) do |f| %>
<p>
    <%= f.label 'Name of Food' %><br>
    <%= f.text_field :name %>
</p>
<p>
    <%= f.label :calories %><br>
    <%= f.text_field :calories %>
</p>
<p>
    <%= f.label :price %><br>
    <%= f.text_field :price %>
</p>
<p>
    <%= f.submit %>
</p>

<% end %>
<%= link_to 'Back', @restaurant %>